{
    "name": "Joker Extension",
    "description": "ジョーカーの漫画を読むのを楽にする",
    "version": "0.1",
    "icons": { "48": "icon48.png",
              "128": "icon128.png" },
    "manifest_version": 2,
    "permissions": [
        "tabs", "http://joker.applibot.co.jp/*"
    ],
    "content_scripts": [
        {
            "matches": ["http://joker.applibot.co.jp/*"],
            "js": ["js/jquery-2.1.4.min.js", "js/joker.js"]
        }
    ]
}
